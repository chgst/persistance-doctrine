{
    "name": "chgst/persistence-doctrine",
    "description": "Doctrine implementation for data store",
    "type": "library",
    "license": "MIT",
    "require": {
        "chgst/common":"*",
        "doctrine/common": "^2"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "spec/"
        }
    },
    "require-dev": {
        "phpspec/phpspec": "^6",
        "friends-of-phpspec/phpspec-code-coverage": "^4.3",
        "php-coveralls/php-coveralls": "^2.2",
        "doctrine/orm": "^2.6",
        "doctrine/mongodb-odm": "^1.2"
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.2"
        }
    }
}
