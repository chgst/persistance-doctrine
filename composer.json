{
    "name": "chgst/persistence-doctrine",
    "description": "Doctrine implementation for data store",
    "type": "library",
    "license": "MIT",
    "require": {
        "chgst/common":"@dev",
        "doctrine/common": "^2"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "spec/"
        }
    },
    "require-dev": {
        "phpspec/phpspec": "^4.0",
        "leanphp/phpspec-code-coverage": "^4.2",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:chgst/common.git"
        }
    ],
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.2"
        }
    }
}
